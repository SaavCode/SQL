/*gets one of each*/
SELECT author_lname FROM books;/*two results for some names*/
 
SELECT DISTINCT author_lname FROM books;/*removes multiple appearances*/
 
SELECT author_fname, author_lname FROM books;
 
SELECT DISTINCT CONCAT(author_fname,' ', author_lname) FROM books;/* returns concatenated but not repeeated*/
 
SELECT DISTINCT author_fname, author_lname FROM books;/*get both rows distinct*/