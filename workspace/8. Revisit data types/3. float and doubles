 /*float and doubles are approximate, decimals are exact*/
  /*float and doubles store larger numbers and use less space but not as accurate*/
CREATE TABLE thingies (price FLOAT);
  /*price 88.45*/
INSERT INTO thingies(price) VALUES (88.45);
  /*88.45*/
SELECT * FROM thingies;
  /**/
INSERT INTO thingies(price) VALUES (8877.45);
  /*8877.45*/
SELECT * FROM thingies;
  /*only stores the first seven digits*/
INSERT INTO thingies(price) VALUES (8877665544.45);
  /*8877670000*/
SELECT * FROM thingies;